export PATH_ROOT="/home/fakename/repos/softproj"
export PATH_C=${PATH_ROOT}/source/c
export PATH_PY=${PATH_ROOT}/source/py
export PATH_SRC=${PATH_ROOT}/debug
export PATH_OUT=${PATH_ROOT}/debug
rm -rf ${PATH_OUT}/*
cd $PATH_ROOT
cp ${PATH_PY}/setup.py ${PATH_OUT}
./scripts/copy
cd $PATH_OUT
python3 setup.py build_ext --inplace
cp *.so ${PATH_PY}
