{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-11 build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-lm",
                "-lpython3.10"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "label": "Build C",
            "command": "~/workspace/softproj/scripts/build_c",
            "args": [],
            "type": "shell",
        },
        {
            "label": "Copy Resources",
            "command": "~/workspace/softproj/scripts/copy",
            "args": [],
            "type": "shell",
        }
    ],
    "version": "2.0.0"
}